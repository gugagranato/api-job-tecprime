{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "products"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "float"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "categoria": {
      "unique": true,
      "model": "categorias"
    }
  }
}
